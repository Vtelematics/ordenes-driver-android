<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white">

    <LinearLayout
        android:id="@+id/layout_restaurant_dob_calendar_title"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_restaurant_dob_calendar_title"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorPrimary"
            android:gravity="center"
            android:padding="4dp"
            android:text="@string/select_date"
            android:textColor="@color/white"
            android:textSize="15sp" />

      <!--  <LinearLayout
            android:id="@+id/layout_restaurant_dob_calendar_close"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.2"
            android:gravity="center"
            android:orientation="vertical"
            android:background="@color/colorPrimary">

            <ImageView
                android:id="@+id/img_restaurant_dob_calendar_close"
                android:layout_width="@dimen/_24sdp"
                android:layout_height="@dimen/_24sdp"
                android:contentDescription="@string/restaurant_language_close_image"
                android:background="@color/colorPrimary"
                android:gravity="center"
                android:src="@drawable/ic_clear_white_24dp" />

        </LinearLayout>-->


    </LinearLayout>

    <CalendarView
        android:id="@+id/calendar_View_restaurant_dob"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:layout_marginTop="42dp"
        android:layout_marginBottom="42dp"/>
    <!--android:layout_marginBottom="@dimen/_42sdp"-->

    <LinearLayout
        android:id="@+id/layout_restaurant_dob_calendar_action"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="@color/white"
        android:layout_gravity="bottom"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_View_restaurant_dob_cancel"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_margin="1dp"
            android:gravity="center"
            android:background="@drawable/bg_user_session_action"
            android:padding="4dp"
            android:text="@string/txt_cancel"
            android:textColor="@color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tv_View_restaurant_dob_submit"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_margin="1dp"
            android:background="@drawable/bg_user_session_action"
            android:gravity="center"
            android:padding="4dp"
            android:text="@string/submit"
            android:textColor="@color/white"
            android:textSize="18sp" />

        </LinearLayout>



